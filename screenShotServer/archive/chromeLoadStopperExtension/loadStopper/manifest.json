{
  "name": "LoadStopper",
  "description": "Stops pageload after set interval",
  "version": "0.1",
  "permissions": [
    "debugger"
  ],
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "script.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
  "manifest_version": 2
}
